
        <div class="portfo">
            <div id="header">
                <a href="/"><span class="logo"><img src="/images/logo-aavo.jpg"></span></a>
                <h1><%= __('Blog') %></h1>
                
            </div>
            <header class="listCatProj major container 75%">
                    <ul class="">
                        <li class="active" rel="all"><%= __('Tous') %></li>
                        <% for(var i=0; i<categories.length; i++) {%>
                            <li rel="<%= categories[i].id %>" >
                                <span><%= categories[i].name %></span>
                            </li>
                        <% } %>
                    </ul>
            </header>
            <div id="main">


                 <% for(var i=0; i<articles.length; i++) {%>
                   
                            
                    <div class="box container collapse in  wow zoomInUp" rel="<% if(articles[i].category){ %><%= articles[i].category.id %><% } %>">
                        <header>
                            <h2><%= articles[i].title %></h2>
                        </header>
                        <div  class="col col-xs-12 ">
 

                                <% if(articles[i].publishVideo==true && articles[i].video){ %>

                                         <!--  <div class="container2">
                                            <div class="vendor">
                                                <iframe src="https://player.vimeo.com/video/127909728?badge=0" width="500" height="281" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
                                              </div>
                                              

                                          </div>   -->
                                            <div class="pretty-embed" data-pe-videoid="<%= articles[i].video %>"></div>
                                <%}else if(articles[i].img) { %>
                                    <img src="/file/image/originalsize/<%= articles[i].img.filename %>" alt="<%= articles[i].img.alt %>"/>
                                <% } %>
                        </div>
                        <div  class="col col-xs-12 text-center">
                            <%- articles[i].content %>
                            
                        </div><div  class="col col-xs-12 text-center">
                            <% if(articles[i].rewriteurl){ %>
                            <a href="article/<%= articles[i].id %>/<%= articles[i].rewriteurl %>" class="button"><%= __('see_article_button') %></a>
                            <% } else {%>
                            <a href="article/<%= articles[i].id %>/" class="button"><%= __('see_article_button') %></a>
                            <% } %>
                        </div>
                    </div>
                    <% } %>
                

            </div>
            <div class="box container toto"> 
                
             


            </div>
        </div>
