
<div class="container-wrapper navigation">
        <nav class="navbar navbar-default" role="navigation">
        
            <div class="container">
            
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="">
                        <i class="fa fa-bars"></i>
                    </button>
                    <a class="navbar-brand" href="/">
                        <img src="images/front/arbatouligne.svg"  />
                    </a>
                    <ul class="mini"></ul><!-- mobile navigation -->
                </div><!-- .navbar-header -->
                
                <div class="collapse navbar-collapse">
                    <div class="right">
                        <ul class="nav navbar-nav">
                            <li class=""><a href="/">Home</a></li>
                            <li class=""><a href="/presta">Prestations</a></li>
                            <li class=""><a href="/noeuds">Nœuds</a></li>
                            <li class="active"><a href="/blog">Blog</a></li>
                            <li class=""><a href="/contact">Contact</a> </li>
                        </ul>
                    </div>
                </div><!-- /.navbar-collapse -->
            </div><!-- /.container-fluid -->
            
        </nav>
    </div>
    <div id="nav-ends"></div>
    

   	<section class="content-section low-rider">
    	<div class="container">
        	<div class="row">
            
            	<div class="col-lg-9 single">
                   	<div class="blog">
                        <div class="imagesblog">
                          <% for(var j =0; j < article.images.length; j++) { %>
                                <% if(article.images[j] && article.images[j].index == 0){ %>
                                 <span class="image anim fadeIn">
                                    <a  href="file/image/originalsize/<%= article.images[j].filename %>" id="desc" data-icon="fa-eye" data-lightbox="<%= article.id %>">
                                        <img src="file/image/thumbs/<%= article.images[j].filename %>" alt="<%=article.title %> - <%=article.rewriteurl %>" />
                                    </a>
                                </span>
                                <% } %>
                                

                              
                            <% } %>
                            <div class="thumbslist">
                            <% for(var j =0; j < article.images.length; j++) { %>
                                <% if(article.images[j] && article.images[j].index !== 0){ %>
                                    <a href="file/image/originalsize/<%= article.images[j].filename %>" id="asc" data-icon="fa-search" data-lightbox="<%= article.id %>">
                                    <img class="thumbs" src="file/image/thumbs/<%= article.images[j].filename %>" alt="<%=article.title %> - <%=article.rewriteurl %>" />
                                    </a>
                                <% } 
                            } %>
                           
                            </div>
                        <!-- .image -->	   
                        </div>  
         
                        <div class="date anim fadeIn">
                            <a href="#" class="day">
                                <%= article.dateJ %>
                            </a>
                            <a href="#" class="month">
                                <%= article.dateM %>
                            </a>
                        </div><!-- .date -->
                        
                        <span class="title-desc anim fadeIn">
                            <h1 ><%= article.title %></h1>
                            <ul class="meta">
                                <li><a href="#"><i class="fa fa-user"></i><%= article.author %></a></li>
                                    <li><a href="#"><i class="fa fa-eye"></i><%= article.nbView %></a></li>
                                    <li><a href="#"><i class="fa fa-th-list"></i>
                                        <% for(var j in category){
                                          if(article.category && category[j].id==article.category.id){%>
                                                <%= category[j].title %>
                                          <% } %>
                                        <% } %>
                                    </a></li>
                            </ul><!-- .meta -->
                        </span><!-- .title-desc -->
                        
                        <p class="content">
                            
                        <%- article.content %>
                        </p>
                    </div><!-- .blog -->
                    
                    <div class="clearfix"></div>
                    
                	
                  	<h4 class="anim fadeIn">
                    	Comments (<%= coments.length %>)
                    </h4>
                   
                 	<ul class="comments">
                        <% for(var i in coments ){ %>
                          
              			<li class="anim fadeIn" data-wow-delay="0.2s">
                        	<div class="wrapper">
                                <!-- <img src="images/front/blog-author-example.jpg" alt="Dale Blog Post Author Example" /> -->
                                <h5>
                                    <%= coments[i].author %>
                                </h5>
                                <span>
                                    <%= coments[i].date %>
                                </span>
                                <p>
                                    <%= coments[i].content %>
                                </p>
                                <!-- <a href="#">
                                    Reply
                                </a> -->
                            </div><!-- .wrapper -->
                        </li>
                        <% } %>
                        
                  	</ul>
				
                	<h6 class="anim fadeIn" data-wow-delay="0.24s">
                    	Laissez-nous un petit commentaire
                    </h6>
                    
                	<div class="form contact postcoment style-2">
                        <form target="#" name="contact">
                            <span class="input-group anim fadeIn" data-wow-delay="0.1s">
                                <i class="fa fa-user"></i>
                                <input type="text" name="contactName" id="author" class="lg" placeholder="Nom" />
                            </span><!-- .input-group -->
                            
                            <span class="input-group anim fadeIn" data-wow-delay="0.20s">
                                <i class="fa fa-envelope"></i>
                                <input type="text" name="contactEmail" id="email" class="lg" placeholder="Address email " />
                            </span><!-- .input-group -->
                            
                            <span class="input-group anim fadeIn" data-wow-delay="0.30s">
                                <textarea name="contactMessage" id="content" class="lg" placeholder="Exprimez-vous!"></textarea>
                            </span><!-- .input-group -->
                            
                            <span id="message"></span>
                            
                           	<a class="btn btn-sm btn-primary icon anim fadeIn" id='comentADD' data-wow-delay=".45s" role="button"><i class="fa fa-long-arrow-right"></i>Poster mon commentaire</a>
                            <span id="message_sent"><i class="fa-check fa"></i></span>
                        </form>
                    </div>

                </div><!-- .col-lg-9 -->
                
              	<div class="col-lg-3 sidebar">
                    
                    
                        <h4 class="anim fadeInRight">Catégories<i class="fa fa-th-list"></i></h4>
                     
                        <div class="anim fadeInRight">
                            <ul class="posts" >
                            <% for(var i in category){ %> 
                                <a href="blog/category/<%= category[i].id %>"><li><h6><%= category[i].title %></h6></li></a>
                            <% } %> 
                            </ul>
                        </div><!-- .text -->
                        
                        <h4 class="anim fadeInRight">
                            Les plus vues<i class="fa fa-bomb"></i>
                        </h4>
                        
                        <div class="anim fadeInRight">
                                    <ul class="posts">

                                        <% for(var i in mostseen){ %> 
                                            <li>
                                                <a href="blog/article/<%= mostseen[i].id %>/<%= mostseen[i].rewriteurl %>">
                                                    
                                                <h6>
                                                    <%= mostseen[i].title %>
                                                </h6>
                                                <cite><i class="fa fa-date"></i><%= mostseen[i].date %></cite>
                                                <div class="clearfix"></div>
                                                </a>

                                            </li>
                                        <% } %> 
                                        
                                       
                                    </ul><!-- .posts-wrapper -->
                        </div>
                    </div>
                    
                </div>
                
            </div><!-- .row -->
        </div><!-- .container -->
    </section><!-- .content-section -->
 <script>
    var coments = <%- JSON.stringify(coments) %>
    console.log(coments);
   
    var article = <%- JSON.stringify(article) %>
    console.log(article);
    

</script>