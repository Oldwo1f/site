   	<section class="content-section low-rider margintop blog">
    	<div class="container">
        	<div class="row">
            
            	<div class="col-xs-12 single">
                   	<div class="blog" style="margin-top:70px;">
                             
                            
                         <% if(project.publishVideo==true && project.video){ %>
                            <div class="pretty-embed" data-pe-videoid="<%= project.video %>"></div>
                            <%} if(project.images.length) { %>
                                
                                <div class="imagesblog" style="margin-top:20px">
                                    <% for(var j =0; j < project.images.length; j++) { %>
                                            <div class="image2">
                                                <img src="/file/image/originalsize/<%= project.images[j].image.filename %>" alt="<%= project.images[j].image.alt %>"/>
                                            </div>
                                    <% } %>
                                </div>
                            <% } %>

             
                            <div class="date" >
                                <a  class="day" title="<%- moment(project.date).fromNow() %>">
                                    <%- moment(project.date).format('D') %>
                                </a>
                                <a  class="month" title="<%- moment(project.date).format('YYYY') %>" >
                                    <%- moment(project.date).format('MMM') %>
                                </a>
                            </div><!-- .date -->
                        
                        <span class="title-desc anim fadeIn">
                            <h1 ><%= project.title %></h1>
                            <ul class="meta">
                                    <% if( project.author ){%> 
                                        <% if(project.author.usename==true){%> 
                                        <li><i class="fa fa-user"></i><%= project.author.name %></li>
                                        <% }else{ %>
                                        <i class="fa fa-user"></i><%= project.author.pseudo %></li>
                                        <% } %>
                                    <% } %>
                                    <li><i class="fa fa-eye"></i><%= project.nbView %></li>
                                    <li>
                                        <% for(var j in category){ %>
                                           
                                         <%  if(project.category && category[j].id==project.category){%>
                                                <a href="blog/category/<%= category[j].id  %>"><i class="fa fa-th-list"></i>
                                                <%= category[j].name %>
                                                </a>
                                          <% } %>
                                        <% } %>
                                    </li>
                                    
                            </ul>
                        </span><!-- .title-desc -->
                        
                        <div class="contenu">
                            
                        <%- marked(project.content) %>

                        </div>
                        <div class="clearfix"></div>
                        <% if(project.documents.length > 0) {%> 
                            <div class="pj">
                                <label><%=  __('PiÃ¨ces jointes') %></label>
                                <ul>
                                    <% for(var i=0; i<project.documents.length; i++) {%>
                                    <li><a href="/file/document/<%= project.documents[i].document.filename %>" target="_BLANK"><%= project.documents[i].document.filename %></a></li>
                                    <% } %>  
                                </ul>
                            </div>
                        <% } %>
                        
                    </div><!-- .blog -->
                     <div id="socials">
                     <div id="example1">
                      <div id="twitter" data-url="<%= domain %>blog/project/<%= project.id %>/<%- project.rewriteurl %>" data-text="<%= project.title %>" data-title="Tweet"></div>
                      <div id="facebook" data-url="<%= domain %>blog/project/<%= project.id %>/<%- project.rewriteurl %>" data-text="<%= project.title %>" data-title="Like"></div>
                    </div>
                    </div>
                    <div class="clearfix"></div>
                    
                	<h4 class="anim fadeIn">
                        <%= __('Commentaires') %> (<%= project.comments.length %>)
                    </h4>
                   
                    
                    
                	<div class="form contact postcoment style-2">
                        <form target="#" name="contact">
                            <span class="input-group anim fadeIn" data-wow-delay="0.1s">
                                <i class="fa fa-user"></i>
                                <input type="text" name="contactName" id="author" class="lg" placeholder="Nom" />
                            </span><!-- .input-group -->
                            
                            <span class="input-group anim fadeIn" data-wow-delay="0.20s">
                                <i class="fa fa-envelope"></i>
                                <input type="text" name="contactEmail" id="email" class="lg" placeholder="Address email " />
                            </span><!-- .input-group -->
                            
                            <span class="input-group anim fadeIn" data-wow-delay="0.30s">
                                <textarea name="contactMessage" id="content" class="lg" placeholder="Exprimez-vous!"></textarea>
                            </span><!-- .input-group -->
                            
                            <span id="message"></span>
                            
                           	<a class="btn btn-sm btn-primary icon anim fadeIn" id='comentprojADD' data-wow-delay=".45s" role="button"><i class="fa fa-long-arrow-right"></i><%= __('Poster mon commentaire') %></a>
                            <span id="message_sent"><i class="fa-check fa"></i></span>
                        </form>
                    </div>

                    <div id="" class="box boxcomment addCommentForm <%= project.id %>">
                           
                        
                        <ul class="comments">
                            <% for(var j=0; j<project.comments.length; j++) {%>
                            <li class="box boxcomment">
                                <h5><%= project.comments[j].author %>  <span class="date pull-right small"><%= moment(new Date(project.comments[j].createdAt)).fromNow() %></span></h5>
                               
                                <p><%= project.comments[j].content %></p>

                                <ul class="comments" >
                                    <% for(var k=0; k<project.comments[j].reponses.length; k++) {%>
                                    <li class="box boxcomment">
                                        <h5 ><%= project.comments[j].reponses[k].author %><span class="date pull-right small"><%= moment(new Date(project.comments[j].reponses[k].createdAt)).fromNow() %></span></h5>
                                        
                                        <p><%= project.comments[j].reponses[k].content %></p>
                                        
                                    </li>
                                    <% } %>
                                </ul>  
                            </li>
                            <% } %>
                        </ul>
                    
                    </div>
                </div><!-- .col-lg-9 -->
                
              	
                
            </div><!-- .row -->
        </div><!-- .container -->
    </section><!-- .content-section -->
