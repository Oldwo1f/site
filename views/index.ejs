<!-- Our Work Section -->
<div id="work" class="page">
  <div class="container">
      <!-- Title Page -->
        <div class="row">
            <div class="span12">
                <div class="title-page">
                    <h2 class="title wow fadeInRight"><%- __('title-project') %></h2>
                    <h3 class="title-description wow fadeInRight" data-wow-delay="0.5s"><%- __('subtitle-project') %></h3>
                </div>
            </div>
        </div>
        <!-- End Title Page -->
        
        <!-- Portfolio Projects -->
        <div class="row">
          <div class="span3">
              <!-- Filter -->
                <nav id="options" class="work-nav">
                    <ul id="filters" class="option-set" data-option-key="filter">
                        <li class="type-work  wow fadeInLeft"><%- __('category-project') %></li>
                        <li class="wow fadeInUp" data-wow-delay="0.5s"><a href="#filter" data-option-value="*" class="selected"><%- __('all projects') %></a></li>
                        <% for(var i=0; i<categories.length; i++) {%>
                        <li class="wow fadeInUp" data-wow-delay="<%= (i+2)*0.5%>s"><a href="#filter" data-option-value=".<%= categories[i].id %>" class=""><%= categories[i].name %></a></li>
                        <% } %>
                    </ul>
                </nav>
                <!-- End Filter -->
            </div>
            
            <div class="span9">
              <div class="row">
                  <section id="projects">
                      <ul id="thumbs">
                        
                          <% for(var i=0; i<projects.length; i++) {%>

                            <% 
                            var url = 'projet/'+projects[i].id + '/';
                            if(projects[i].rewriteurl){ 
                              url = 'projet/'+projects[i].id + '/'+projects[i].rewriteurl; 
                            } 
                            %>
                            <li data-wow-delay="<%= (i)*0.5%>s" class="item-thumbs span3 wow zoomInUp <% if(projects[i].category){ %><%= projects[i].category.id %><% } %>">
                              <!-- Fancybox - Gallery Enabled - Title - Full Image -->
                                <a class="hover-wrap fancybox"  title="<%= projects[i].title %>" content=" " href="<%= url %>">
                                  <span class="overlay-img"></span>
                                    <span class="overlay-img-thumb font-icon-eye"></span>
                                </a>
                                <!-- Thumb Image and Description -->
                                <% if(projects[i].img){ %>
                                <img src="/file/image/adminThumbs/<%= projects[i].img.filename %>" alt="TOTOTOTO <%= projects[i].img.alt %>"/><% } %>
                            </li>
                          <% } %>
                          
                        </ul>
                    </section>
                    
              </div>
            </div>
        </div>
        <!-- End Portfolio Projects -->
    </div>
</div>
<!-- End Our Work Section -->

<!-- About Section -->
<div id="about" class="page-alternate">
<div class="container">
    <!-- Title Page -->
    <div class="row">
        <div class="span12">
            <div class="title-page">
                <h2 class="title wow fadeInRight"><%- __('title-news') %></h2>
                <h3 class="title-description wow fadeInRight" data-wow-delay="0.5s"><%- __('subtitle-news') %></h3>
            </div>
        </div>
    </div>
    <div class="row">

        <% for(var i=0; i<articles.length; i++) {%>

        <% 
            var url2 = 'article/'+articles[i].id + '/';
            if(articles[i].rewriteurl){ 
              url2 = 'article/'+articles[i].id + '/'+articles[i].rewriteurl; 
            } 
            %>
        <div class="span4 profile wow fadeInUp" data-wow-delay="<%= (i)*0.5%>s">
          <div class="image-wrap ">
                <a class="hover-wrap fancybox2"  title="<%= projects[i].title %>" content=" " href="<%= url2 %>">
                    <span class="overlay-img"></span>
                    <span class="overlay-text-thumb"><span class="font-icon-eye"></span> Voir l'article</span>
                </a>
                 <% if(articles[i].img){ %>
                    <img src="/file/image/originalsize/<%= articles[i].img.filename %>" alt="<%= articles[i].img.alt %>" />
                  <% } %>
            </div>
            <h3 class="profile-name"><%= articles[i].title %></h3>
            <p class="profile-description"><%- marked(articles[i].content) %><a class="fancybox2"  title="<%= projects[i].title %>" content=" " href="<%= url2 %>"><%- __('Lire_la_suite') %></a></p>
                

              
            <!-- <div class="social">
              <ul class="social-icons">
                  <li><a href="#"><i class="font-icon-social-twitter"></i></a></li>
                    <li><a href="#"><i class="font-icon-social-dribbble"></i></a></li>
                    <li><a href="#"><i class="font-icon-social-facebook"></i></a></li>
                </ul>
                
                
            </div> -->
        </div>
        <% } %>

    
        
    </div>
</div>
</div>
<!-- End About Section -->


<!-- Contact Section -->
<div id="contact" class="page">
<div class="container">
    <!-- Title Page -->
    <div class="row">
        <div class="span12">
            <div class="title-page">
                <h2 class="title wow fadeInRight"><%- __('title-contact') %></h2>
                <h3 class="title-description wow fadeInRight" data-wow-delay="0.5s"><%- __('subtitle-contact') %></h3>
            </div>
        </div>
    </div>
    <!-- End Title Page -->
    
    <!-- Contact Form -->
    <div class="row">
      <div class="span9">
        
          <form id="contact-form" class="contact-form wow fadeInLeft" data-wow-delay="0.5s">
              <p class="contact-name">
                <input id="contact_name" type="text" placeholder="<%- __('form-full_name') %>" value="" name="name" />
                </p>
                <p class="contact-email">
                  <input id="contact_email" type="text" placeholder="<%- __('form-email_adress') %>" value="" name="email" />
                </p>
                <p class="contact-message">
                  <textarea id="contact_message" placeholder="<%- __('form-message') %>" name="message" rows="15" cols="40"></textarea>
                </p>
                
                <div class="response">
                        <div class="row errorMessageRequired">
                            <div class="12u text-center">
                                <small class=" label label-danger"><%- __('contact_all_required') %></small>
                            </div>
                        </div>
                        <div class="row errorMessageError">
                            <div class="12u text-center">
                                <small class=" label label-danger"><%- __('contact_error_message') %></small>
                            </div>
                        </div>
                        <div class="row SuccessMessage">
                            <div class="12u text-center">
                                <small class=" label label-success"><%- __('contact_success_message') %></small>
                            </div>
                        </div>
                </div>
                <p class="contact-submit wow fadeInRight" data-wow-delay="1.5s" >
                  <input id="contact-submit" type="submit" class="submit" value="<%- __('form-send-button') %>"></input>
                </p>
            </form>
         
        </div>
        
        <div class="span3">
          <div class="contact-details wow fadeInRight" data-wow-delay="0.5s">
            <h3><%- __('contact-details-title') %></h3>
                <ul>
                    <li><a href="#"><%- __('contact-details-email') %></a></li>
                    <li><%- __('contact-details-phone') %></li>
                    <li>
                        <%- __('adresse_l1') %>
                        <br>
                        <%- __('adresse_l2') %>
                        <br>
                        <%- __('adresse_l3') %>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <!-- End Contact Form -->
</div>
</div>
<!-- End Contact Section -->


