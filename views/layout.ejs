<script>
$(document).ready(function() {

        $('#clientCarousel').carousel({
      interval: 4000
    })

    $('#clientCarousel.carousel .item').each(function(){
      var next = $(this).next();
      if (!next.length) {
        next = $(this).siblings(':first');
      }
      next.children(':first-child').clone().appendTo($(this));
      
      for (var i=0;i<2;i++) {
        next=next.next();
        if (!next.length) {
            next = $(this).siblings(':first');
        }
        
        next.children(':first-child').clone().appendTo($(this));
      }
    })
        $('a.ajax').click(function(e){
        $('.contentWrapper').html('<div class="maincontainer"></div>')
        e.preventDefault();
        console.log();
        var H = $('.refheight').height();
        console.log(H);
        $('.maincontainer').load($(this).attr('href')).css('height', H-9)
    });

});
</script>
